<h1>New Post</h1>

<%= form_for @post do |f| %>
  <fieldset>
    <%= f.label :title %>
    <%= f.text_field :title, :required => true %>
  </fieldset>

  <fieldset>
    <%= f.label :link %>
    <%= f.text_field :link, :required => true %>
  </fieldset>

  <!-- <fieldset>
    <%# f.label :body %>
    <%# f.text_area :body, :required => true %>
  </fieldset> -->

  <%= f.submit %>
<% end %>
